<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.mattalui.autologs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewLogs">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateLogActivity"
            android:parentActivityName=".ViewLogs">
        </activity>
        <activity
            android:name=".Login">
        </activity>
        <activity
            android:name=".SignUp"
            android:parentActivityName=".Login">
        </activity>
        <activity
            android:name=".ViewVehicles"
            android:parentActivityName=".ViewLogs">
        </activity>
        <activity
            android:name=".CreateVehicleActivity"
            android:parentActivityName=".ViewVehicles">
        </activity>
        <activity
            android:name=".ViewStatistics"
            android:parentActivityName=".ViewLogs">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>